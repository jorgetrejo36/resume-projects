<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%=homeStartingContent%></p>

<% posts.forEach((post) => { %>
    <h1><%=post.title%></h1>
    <p><%= _.truncate(post.content, {length: 103}) %> 
        <a href="posts/<%=post._id%>">Read More</a>
    </p>
<% }); %>

<%- include('partials/footer'); -%>